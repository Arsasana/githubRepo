<?xml version="1.0" encoding="gb2312"?>
<UFEntityModel isvisible="yes">
	<!---------------------------------风控内部数据 -------------------------------------------------->
	<!---------------------------------基础-框架 -------------------------------------------------->
	<Entity name="InstitutionUser" title="机构用户" description="机构用户">
		<Field name="InstitutionID" type="InstitutionID" notnull="yes" label="机构代码" description="机构代码" iskey="yes" />
		<Field name="UserID" type="UserID" notnull="yes" label="用户代码" description="用户代码" iskey="yes"/>
		<Field name="UserName" type="UserName" label="用户名称" description="用户名称"/>
		<Field name="UserType" type="UserType" label="用户类型" description="用户类型"/>
		<Field name="Password" type="Password" notnull="yes" label="密码" description="每一位用户进入交易系统的口令"/>		
		<Field name="IsActive" type="Bool" notnull="yes" label="是否活跃" description="表示该用户是否活跃"/>
	</Entity>
	
	<Entity name="InstitutionUserFunction" title="机构用户功能权限" description="机构用户功能权限">
		<Field name="InstitutionID" type="InstitutionID" notnull="yes" label="机构代码" description="机构代码" iskey="yes" />
		<Field name="UserID" type="UserID" notnull="yes" label="用户代码" description="用户代码" iskey="yes"/>
		<Field name="InstitutionFunctionCode" type="InstitutionFunctionCode" notnull="yes" label="机构功能代码" description="机构功能代码" iskey="yes"/>
	</Entity>


	
	<!---------------------------------基础-业务-------------------------------------------------->
	<Entity name="Investor" title="投资者" description="投资者">
		<Field name="InstitutionID" type="InstitutionID" notnull="yes" label="机构代码" description="机构代码" iskey="yes" />
		<Field name="InvestorID" type="InvestorID" notnull="yes" label="投资者代码" description="投资者代码" iskey="yes" />
		<Field name="InvestorName" type="PartyName" notnull="yes" label="投资者名称" description="投资者名称" />
		<Field name="IsActive" type="Bool" notnull="yes" label="是否活跃" description="表示该客户是否活跃" />
		<Field name="Telephone" type="Telephone" label="联系电话" description="联系电话" />
		<Field name="Address" type="Address" label="通讯地址" description="通讯地址" />
		<Field name="Mobile" type="Mobile" label="手机" description="手机" notnull="no"/>
		<Field name="Fax" label="传真" type="Fax" description="传真" notnull="no" />
		<Field name="EMail" label="电子邮件" type="EMail" description="电子邮件" notnull="no" />
		<Field name="OpenDate" type="Date" label="开户日期" description="开户日期" notnull="no"/>
	</Entity>
	
	
	
	<!--todo 交易所ID是否可以改为枚举值，不要客户自定义 -->
	<Entity name="Exchange" title="交易所" description="交易所的基本信息">
		<Field name="ExchangeID" type="ExchangeID" notnull="yes" label="交易所代码" description="一个交易所的编号" iskey="yes" />
		<Field name="ExchangeName" type="ExchangeName" notnull="yes" label="交易所名称" description="交易所名称" />
	</Entity>
	
	<Entity name="Product" title="产品" description="有相同基础品种和衍生种类（即期货、期权或期货期权）的一组合约">
		<Field name="ExchangeID" type="ExchangeID" notnull="yes" label="交易所代码" description="一个交易所的编号" iskey="yes" />
		<Field name="ProductID" type="InstrumentID" notnull="yes" label="产品代码" description="交易品种在系统中的编号" iskey="yes" />
		<Field name="ProductName" type="ProductName" notnull="yes" label="产品名称" description="产品名称" />
		<Field name="ProductClass" type="ProductClass" notnull="yes" label="产品类型" description="产品类型（1．期货2．期权3．组合4．即期）" />
		<Field name="VolumeMultiple" type="VolumeMultiple" notnull="yes" label="合约数量乘数" description="品种的每手交易的规定数量，针对基础的商品" />
		<Field name="PriceTick" type="Price" notnull="yes" label="最小变动价位" description="合约的单位价格涨跌变化的最小值" />
		<Field name="PositionType" type="PositionType" notnull="yes" label="持仓类型" description="说明是综合持仓还是净持仓"/>
		<Field name="PositionDateType" type="PositionDateType" notnull="yes" label="持仓日期类型" description="说明是否区分今日仓和历史仓"/>
	</Entity>
	
	<Entity name="Instrument" title="合约" description="某一特定种类（即期货、期权或期货期权）的一个衍生品种">
		<Ref entity="Product"/>
		<Field name="InstrumentID" type="InstrumentID" notnull="yes" label="合约代码" description="合约在系统中的编号" iskey="yes" />
		<Field name="InstrumentName" type="InstrumentName" notnull="yes" label="合约名称" description="合约的名称" />
		<Field name="ExchangeInstID" type="ExchangeInstID" notnull="yes" label="合约在交易所的代码" description="合约在交易所的代码" />
		<Field name="DeliveryYear" type="Year" label="交割年份" description="该合约规定进行实物交割的年份" />
		<Field name="DeliveryMonth" type="Month" label="交割月" description="该合约规定进行实物交割的月份" />
		<Field name="CreateDate" type="Date" label="创建日" description="新合约创建时间" />
		<Field name="OpenDate" type="Date" label="上市日" description="上市日" />
		<Field name="ExpireDate" type="Date" label="到期日" description="合约到期时间" />
		<Field name="StartDelivDate" type="Date" label="开始交割日" description="了结到期未平仓合约的开始日期" />
		<Field name="EndDelivDate" type="Date" label="结束交割日" description="了结到期未平仓合约的结束日期" />
		<Field name="InstLifePhase" type="InstLifePhase" label="合约生命周期状态" description="分为合约未上市、上市、停牌、到期4个状态" />
		<Field name="IsTrading" type="Bool" label="当前是否交易" description="当前是否交易" />
	</Entity>
	
	<Entity name="Institution" title="机构" description="机构">
		<Field name="InstitutionID" type="InstitutionID" notnull="yes" label="机构代码" description="机构代码" iskey="yes" />
		<Field name="InstitutionAbbr" type="InstitutionAbbr" notnull="yes" label="机构简称" description="机构简称" iskey="no" />
		<Field name="InstitutionName" type="InstitutionName" notnull="yes" label="机构名称" description="机构名称" iskey="no" />
		<Field name="IsActive" type="Bool" notnull="yes" label="是否活跃" description="表示该客户是否活跃" />
	</Entity>
	


	<!---------------------------------交易-资金-------------------------------------------------->
	<Entity name="TradingAccount" title="交易资金账户信息" description="说明在交易时使用的资金帐号信息">
		<Field name="InstitutionID" type="InstitutionID" notnull="yes" label="机构代码" description="机构代码" iskey="yes"/>
		<Field name="AccountID" type="AccountID" notnull="yes" label="投资者帐号" description="投资者帐号" iskey="yes" />
		<Field name="InvestorID" type="InvestorID" notnull="yes" label="投资者代码" description="投资者代码" />
		<Field name="PreMortgage" type="Money" notnull="yes" label="上次质押金额" description="上次质押金额" />
		<Field name="PreCredit" type="Money" notnull="yes" label="上次信用额度" description="上次信用额度" />
		<Field name="PreBalance" type="Money" notnull="yes" label="上次结算准备金" description="上次结算准备金" />
		<Field name="PreMargin" type="Money" notnull="yes" label="上次占用的保证金" description="上次占用的保证金" />
		<Field name="Deposit" type="Money" notnull="yes" label="入金金额" description="当日入金的总额" />
		<Field name="Withdraw" type="Money" notnull="yes" label="出金金额" description="当日出金的总额" />
		<Field name="FrozenMargin" type="Money" notnull="yes" label="冻结的保证金" description="因为下单而冻结的保证金" />
		<Field name="FrozenCash" type="Money" notnull="yes" label="冻结的资金" description="因为下单而冻结的资金，包括买入现货的资金和买入期权的权利金" />
		<Field name="FrozenCommission" type="Money" notnull="yes" label="冻结的手续费" description="因为下单而冻结的手续费" />
		<Field name="CurrMargin" type="Money" notnull="yes" label="当前保证金总额" description="当前所有持仓所需要占用的保证金" />
		<Field name="Commission" type="Money" notnull="yes" label="手续费" description="成交单产生的手续费" />
		<Field name="CloseProfit" type="Money" notnull="yes" label="平仓盈亏" description="平仓造成的盈亏金额" />
		<Field name="PositionProfit" type="Money" notnull="yes" label="持仓盈亏" description="当前所有持仓造成的盈亏金额"/>
		<Field name="Balance" type="Money" notnull="yes" label="期货结算准备金" description="等于PreBalance+CloseProfit+PositionProfit+Premium+Deposit－Withdraw-Commission" />
		<Field name="Available" type="Money" notnull="yes" label="可用资金" description="等于Balance-CurrMargin-FrozenMargin-FrozenCommission-FrozenCash" />
		<Field name="WithdrawQuota" type="Money" notnull="yes" label="可取资金" description="等于Fund-(PositionProfit>0?0:PositionProfit)-PreMortgate-PreCredit" />
		<Field name="Mortgage" type="Money" notnull="yes" label="质押金额" description="质押金额"/>
		<Field name="ExchangeMargin" type="Money" notnull="yes" label="交易所保证金" description="交易所保证金"/>
		<Field name="DeliveryMargin" type="Money" notnull="yes" label="投资者交割保证金" description="投资者交割保证金"/>
		<Field name="ExchangeDeliveryMargin" type="Money" notnull="yes" label="交易所交割保证金" description="交易所交割保证金"/>
	</Entity>
	
	<!---------------------------------交易-持仓-------------------------------------------------->
	<Entity name="InvestorPosition" title="投资者合约持仓" description="说明投资者当前持仓信息">
		<Field name="InstitutionID" type="InstitutionID" notnull="yes" label="机构代码" description="机构代码" iskey="yes"/>
		<Field name="InvestorID" type="InvestorID" notnull="yes" label="投资者代码" description="投资者代码" iskey="yes"/>
		<Field name="ExchangeID" type="ExchangeID" notnull="yes" label="交易所代码" description="一个交易所的编号" iskey="yes" />
		<Field name="InstrumentID" type="InstrumentID" notnull="yes" label="合约代码" description="合约在系统中的编号" iskey="yes"/>
		<Field name="PosiDirection" type="PosiDirection" notnull="yes" label="持仓多空方向" description="1．净2．多3．空" iskey="yes"/>
		<Field name="HedgeFlag" type="HedgeFlag" notnull="yes" label="投机套保标志" description="1．投机2．套保" iskey="yes"/>
		<Field name="PrePosition" type="Volume" notnull="yes" label="上日持仓" description="上日持仓的当前数量"/>
		<Field name="OpenVolume" type="Volume" notnull="yes" label="开仓量" description="累计的开仓量"/>
		<Field name="CloseVolume" type="Volume" notnull="yes" label="平仓量" description="累计的平仓量"/>
		<Field name="Position" type="Volume" notnull="yes" label="今日持仓" description="今日持仓的当前数量"/>
		<Field name="LongFrozen" type="Volume" notnull="yes" label="多头冻结" description="多头冻结的持仓数量"/>
		<Field name="ShortFrozen" type="Volume" notnull="yes" label="空头冻结" description="空头冻结的持仓数量"/>
		<Field name="LongFrozenAmount" type="Money" notnull="yes" label="开仓冻结金额" description="多头报单冻结的金额"/>
		<Field name="ShortFrozenAmount" type="Money" notnull="yes" label="开仓冻结金额" description="空头报单冻结的金额"/>
		<Field name="OpenAmount" type="Money" notnull="yes" label="开仓金额" description="累计的开仓金额"/>
		<Field name="CloseAmount" type="Money" notnull="yes" label="平仓金额" description="累计的平仓金额"/>
		<Field name="PositionCost" type="Money" notnull="yes" label="持仓成本" description="当前所有持仓的持有成本"/>
		<Field name="PreMargin" type="Money" notnull="yes" label="上次占用的保证金" description="上次所有持仓占用的保证金"/>
		<Field name="UseMargin" type="Money" notnull="yes" label="占用的保证金" description="所有持仓占用的保证金"/>
		<Field name="FrozenMargin" type="Money" notnull="yes" label="冻结的保证金" description="所有冻结持仓占用的保证金"/>
		<Field name="FrozenCash" type="Money" notnull="yes" label="冻结的资金" description="因为下单而冻结的资金，包括买入现货的资金和买入期权的权利金" />
		<Field name="FrozenCommission" type="Money" notnull="yes" label="冻结的手续费" description="因为下单而冻结的手续费" />
		<Field name="Commission" type="Money" notnull="yes" label="手续费" description="成交单产生的手续费" />
		<Field name="CloseProfit" type="Money" notnull="yes" label="平仓盈亏" description="平仓造成的盈亏金额" />
		<Field name="PositionProfit" type="Money" notnull="yes" label="持仓盈亏" description="当前所有持仓造成的盈亏金额"/>
		<Field name="PreSettlementPrice" type="Price" notnull="yes" label="上次结算价" description="上次的结算价"/>
		<Field name="SettlementPrice" type="Price" notnull="yes" label="本次结算价" description="本次的结算价"/>
		<Field name="OpenCost" type="Money" notnull="yes" label="开仓成本" description="当前合约的开仓成本"/>
		<Field name="ExchangeMargin" type="Money" notnull="yes" label="交易所保证金" description="交易所保证金"/>
		<Field name="CombPosition" type="Volume" notnull="yes" label="组合成交形成的持仓" description="组合成交形成的持仓数量"/>
		<Field name="CombLongFrozen" type="Volume" notnull="yes" label="组合多头冻结" description="组合多头冻结的持仓数量"/>
		<Field name="CombShortFrozen" type="Volume" notnull="yes" label="组合空头冻结" description="组合空头冻结的持仓数量"/>
		<Field name="CloseProfitByDate" label="逐日盯市平仓盈亏" type="Money" description="逐日盯市平仓盈亏" notnull="yes"/>
		<Field name="CloseProfitByTrade" label="逐笔对冲平仓盈亏" type="Money" description="逐笔对冲平仓盈亏" notnull="yes"/>
		<Field name="TodayPosition" type="Volume" notnull="yes" label="今日持仓" description="今日持仓"/>
		<Field name="MarginRateByMoney" type="Ratio" notnull="no" label="保证金率" description="保证金率(按金额)"/>
		<Field name="MarginRateByVolume" type="Ratio" notnull="no" label="保证金率(按手数)" description="保证金率(按手数)"/>
	</Entity>
	
